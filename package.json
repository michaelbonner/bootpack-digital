{
  "name": "bootpack-digital",
  "description": "Public website for Bootpack Digital, LLC",
  "version": "1.0.0",
  "author": "Michael Bonner <michaelwbonner@gmail.com>",
  "dependencies": {
    "formik": "^2.0.8",
    "gatsby": "^2.18.12",
    "gatsby-plugin-google-tagmanager": "^2.1.20",
    "gatsby-plugin-manifest": "^2.2.33",
    "gatsby-plugin-offline": "^3.0.29",
    "gatsby-plugin-postcss": "^2.1.18",
    "gatsby-plugin-purgecss": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-sitemap": "^2.2.24",
    "graphql": "^14.5.8",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "yup": "^0.27.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different '**/*.{js,css,md,json}'",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-eslint": "^10.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^6.7.2",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "prettier": "^1.19.1",
    "tailwindcss": "^1.1.4",
    "typescript": "^3.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelbonner/bootpack-digital"
  },
  "bugs": {
    "url": "https://github.com/michaelbonner/bootpack-digital/issues"
  }
